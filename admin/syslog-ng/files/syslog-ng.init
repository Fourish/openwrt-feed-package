#!/bin/sh /etc/rc.common
# Copyright (C) 2006-2016 OpenWrt.org

START=20
USE_PROCD=1
PROG=/usr/sbin/syslog-ng

config_file=/etc/syslog-ng.conf

start_service() {
	procd_open_instance
	procd_set_param command $PROG -F -f $config_file
	procd_close_instance
}

reload_service() {
	procd_send_signal syslog-ng
}
